package dmaps.study.mentoring3;

public class Unit {
	private String name;
	private int hp;
	private int dp;
	private int power;
	
	
	
	public String getName() {
		
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getHp() {
		return hp;
	}
	public void setHp(int hp) {
		this.hp = hp;
	}
	public int getDefence() {
		return dp;
	}
	public void setDefence(int defence) {
		this.dp = defence;
	}
	public int getPower() {
		return power;
	}
	public void setPower(int power) {
		this.power = power;
	}
	
}
